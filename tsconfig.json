{
    "compilerOptions": {
      "outDir": "./.tsbuild/",
      "sourceMap": false,
      "target": "es2020",
      "module": "commonjs",
      "removeComments": true,
      "noImplicitAny": true,
      "strictPropertyInitialization": true,
      "strictNullChecks": true,
      "types": [
        "node",
        "@wdio/mocha-framework",
        "@wdio/sync"
      ],
    },
    "include": [
      "./test/**/*.ts"
    ],
    "exclude": [
        "./node_modules"
      ]
  }